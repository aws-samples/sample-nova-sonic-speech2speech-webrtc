{
  "homepage": "./",
  "name": "nova-s2s-voice-chat-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "6.1.1",
    "@cloudscape-design/components": "^3.0.498",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.14",
    "amazon-kinesis-video-streams-webrtc": "^2.4.1",
    "aws-amplify": "^6.0.12",
    "aws-sdk": "^2.1691.0",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.2.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "dev": "npm start",
    "serve": "npx serve -s build -l 3000",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "npm run build -- --stats && npx webpack-bundle-analyzer build/static/js/*.js",
    "clean": "rm -rf build node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "check-env": "node scripts/check-env.js",
    "prebuild": "npm run check-env",
    "deploy": "node scripts/deploy.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },

  "babel": {
    "presets": [
      "react-app"
    ]
  },

  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@types/node": "^18.11.18",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "serve": "^14.2.1",
    "source-map-support": "^0.5.21",
    "typescript": "^4.9.4",
    "webpack-bundle-analyzer": "^4.9.1"
  }
