# Nova S2S WebRTC Client Configuration Template
# Copy this file to .env and update the values as needed

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# AWS Configuration
REACT_APP_AWS_REGION=ap-northeast-1
REACT_APP_AWS_ACCESS_KEY_ID=your_access_key_here
REACT_APP_AWS_SECRET_ACCESS_KEY=your_secret_key_here
REACT_APP_AWS_SESSION_TOKEN=        # if no session token, delete this row

# KVS WebRTC Configuration
REACT_APP_KVS_CHANNEL_NAME=nova-s2s-webrtc-test

# Development Server Configuration (optional)
# PORT=3000
# HOST=localhost

# Production Configuration
# CI=false
# DISABLE_ESLINT_PLUGIN=false

# Performance Configuration
# FAST_REFRESH=true